{
  "name": "@manapps/eslint-plugin-manapps",
  "version": "0.0.2",
  "description": "Rules for custom angular projects",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "Matthieu Narcisi",
  "main": "./lib/index.js",
  "exports": "./lib/index.js",
  "scripts": {
    "lint": "npm-run-all \"lint:*\"",
    "lint:eslint-docs": "npm-run-all \"update:eslint-docs -- --check\"",
    "lint:js": "eslint .",
    "test": "jest",
    "build": "tsc && cp lib/index.js dist/lib/ && cp package.json dist/ && cp README.md dist/",
    "update:eslint-docs": "eslint-doc-generator"
  },
  "dependencies": {
    "@typescript-eslint/parser": "8.2.0",
    "@typescript-eslint/rule-tester": "8.2.0",
    "@typescript-eslint/utils": "8.2.0",
    "jest": "29.7.0",
    "requireindex": "^1.2.0",
    "ts-jest": "29.2.5",
    "typescript": "5.5.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.1.0",
    "@eslint/js": "9.9.1",
    "eslint": "9.9.1",
    "eslint-doc-generator": "1.7.1",
    "eslint-plugin-eslint-plugin": "6.2.0",
    "eslint-plugin-n": "17.10.2",
    "globals": "15.9.0",
    "npm-run-all": "4.1.5"
  },
  "engines": {
    "node": ">= 18.0.0"
  },
  "peerDependencies": {
    "eslint": ">=7"
  },
  "license": "ISC"
}
